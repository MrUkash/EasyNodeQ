    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.10",
    "@types/lodash": "^4.14.121",
    "@types/node": "^11.9.4",
    "npm-check": "^5.9.0",
    "typescript": "^3.3.3"
  }
}
